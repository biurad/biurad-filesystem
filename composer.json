{
    "name": "biurad/biurad-flysystem",
    "type": "library",
    "description": "BiuradPHP Framework FileManager for handling filesytem in php and cloud",
    "keywords": [
        "FileManager",
        "filesystem",
        "theleague flysytem",
        "biuradphp",
        "php7"
    ],
    "homepage": "https://www.biurad.com",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Divine Niiquaye Ibok",
            "email": "divineibok@gmail.com"
        },
        {
            "name": "Biurad Lap Community",
            "homepage": "https://biurad.com/contributors"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "league/flysystem": "^1.0",
        "league/flysystem-cached-adapter": "^1.0",
        "tightenco/collect": "^6.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "BiuradPHP\\FileManager\\": "src/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "BiuradPHP\\FileManager\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "suggest": {
        "biurad/nette-di-bridge": "For autowiring the dependency in container"
    }
}
